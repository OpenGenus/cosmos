import java.util.*;
class add1 {
    public static void main(String args[]){
        Scanner scn=new Scanner(System.in);
       //n denotes the no of elements in an array
        int n=scn.nextInt();
        int[] arr=new int[n];
        //x is the value which is to be added to the elemnets of array it can be 1 or any no.
        int x=scn.nextInt();
        for(int i=0;i<n;i++){
            arr[i]=scn.nextInt();
        }
        //function add will return an  array with updated values.
        int[] fans=new int[n];
        fans=add(arr,x);
        //this loop will print the values returned by add function
        for(int a:fans){
            System.out.print(a+" "); //for each loop
        }
        addrec(arr,0,x);
    }
    public static int[] add(int[]arr,int x){
    int[] ans=new int[arr.length];
     for(int i=0;i<arr.length;i++){
         ans[i]=arr[i]+x;
     }
     return ans;
    }
    //recursive functiom
    public static void addrec(int[]arr,int idx,int x){
        if(idx==arr.length){
            return;
        }
        arr[idx]=arr[idx]+x;
        System.out.print(arr[idx]+" ");
        addrec(arr,idx+1,x);
    }
}
