#include <iostream>
using namespace std;



int gcd(int n, int m){
    //find the gcd of given two numbers
     int q=0,r=0;
     max=find_max(n,m);
    min=m+n-max;
     q=max/min;
     r=max-min*q;
     if (r==0)return min;
     else return gcd(min,r);
 }
 

 int find_max(int a,int b){
     //return the largest number between a and b
     if(a>=b)return a;
     else return b;
 }
 
 void are_coprime(int a,int b){
     if(gcd(a,b)==1){
         cout << "coprimes!";
     }
     else
     cout << "have a common divisor";
     
 }
 
 
 
