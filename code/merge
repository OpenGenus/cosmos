#include <stdio.h>
int *
merge (int *A, int *B)
{
  int Asize = sizeof (A);
  int Bsize = sizeof (B);
  int Csize = Asize + Bsize;
  int C[Csize];
  int i=0;
  int j=0;
  
  for (i = 1; i < Asize; i++)
    {
      C[i] = A[i];
    }
  for (j = i - Asize; j < Bsize; j++)
    {
      C[j] = B[j];
    }

  return C;
}

int
main ()
{
  int A[2] = { 19, 10 };
  int B[5] = { 19, 10, 8, 17, 9 };
  merge (A,B);

  return 0;
}
