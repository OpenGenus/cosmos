#include<bits/stdc++.h>
using namespace std;
#define mod 1000000007
#define ull long long int
//using temporary stack 
stack<int> sort(stack<int>s)
{
	if(s.size()<=1)
	return s;
	stack<int>temp;
	int t;
	while(!s.empty())
	{
		t=s.top();
		s.pop();
		while(!temp.empty()&&t<temp.top())
		{
		    s.push(temp.top());
			temp.pop();	
		}
		temp.push(t);
	}
	return temp;
}
int main()
{
	int arr[]={40,-5,6,9,8,10,-9};
	int n=(sizeof(arr)/sizeof(arr[0]));
	stack<int>a;
	for(int i=0;i<n;i++)
	a.push(arr[i]);
	a=sort(a);
	while(!a.empty())
	{
	cout<<a.top()<<" ";
	a.pop();
}

}
